    <!-- /.navbar -->
    <!-- Main Sidebar Container -->
    <%- include('../../_partials/_admin/sidebar.ejs'); %>
    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
      <!-- Content Header (Page header) -->
      <section class="content-header">
        <div class="container-fluid">
          <div class="row mb-2">
            <div class="col-sm-6">
              <h1>Product Price</h1>
            </div>
            <div class="col-sm-6">
              <ol class="breadcrumb float-sm-right">
                <li class="breadcrumb-item"><a href="#">Home</a></li>
                <li class="breadcrumb-item active">Product Price</li>
              </ol>
            </div>
          </div>
        </div>
        <!-- /.container-fluid -->
      </section>
      <!-- Main content -->
      <section class="content">
        <div class="container">
          <!-- SELECT2 EXAMPLE -->
          <div class="card card-primary">
            <div class="card-header">
              <h3 class="card-title">Product Price</h3>
              <div class="card-tools">
                <button type="button" class="btn btn-tool" data-card-widget="collapse">
                  <i class="fas fa-minus"></i>
                </button>
              </div>
            </div>
            <!-- /.card-header -->
            <div class="card-body">
              <form id="productForm" method="post" action="/admin/product_price/create">
                             <input type="text" name="abc" value="1231" />  
                   <div class="row">
                    <table class="table table-striped">
                        <thead>
                          <tr>
                     
                            <th>Store</th>
                            <th>Deals</th>
                            <th>Deal Type</th>
                            <th>Deal Price</th>
                            <th>Regular Price</th>
                            <th>From</th>
                            <th>Till</th>
                          </tr>
                        </thead>
                        <tbody>
                           <% stores.forEach( function (store){ %>
                            <tr>
                                
                                <td><%=store.name%></td>
                                <td>
                                    <select name="deal[]" class="form-control custom-select" id="deals">
                                    <option value="">Select Deal</option>
                                    <% if(deals) deals.forEach(function(deal){ %>
                                      <option value="<%=deal._id%>"><%=deal.name%></option>
                                    <% }) %>
                                  </select>
                                </td>
                                <td>
                                    <select name="deal_value[]" class="form-control custom-select">
                                        <option value="">Deal Value</option>
                                        <option value="percent">Percentage</option>
                                        <option value="price">Price</option>
                                      </select>
                                </td>
                                <td> <input type="text" name="deal_price[]" class="form-control"> </td>
                                <td> <input type="text" name="regular_price[]" class="form-control"> </td>
                                <td> <input type="date" name="stime[]" class="form-control"> </td>
                                <td> <input type="date" name="etime[]" class="form-control"> </td>
                              </tr>
                          <% }) %>
                          
                        
                        </tbody>
                      </table>
                </div>
                <!-- /.card-body -->
                <div class="card-footer">
                  <button type="submit" class="btn btn-primary">Submit</button>
                </div>
              </form>
            </div>
            <!-- /.card -->
          </div>
          <!-- /.container-fluid -->
      </section>
      <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->
    <%- include('../../_partials/_admin/footer.ejs'); %>
